	F.Intrinsic.String.Build("GRP_NAME = '{0}' and PATH = '{1}' and FILE = '{2}'",V.Screen.F_Attach!ddlGroups.Text,V.Local.sPath,V.Local.sFile,V.Local.sFilter)
	F.Data.DataView.SetFilter("dtDocLinks","dvFileCheck",V.Local.sFilter)
